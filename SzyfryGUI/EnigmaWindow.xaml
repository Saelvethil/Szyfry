<Window x:Class="SzyfryGUI.EnigmaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        Title="EnigmaWindow"
        Width="850" Height="370" Closing="Window_Closing">
    <DockPanel Name="DockPanel1" LastChildFill="true">
        <WindowsFormsHost  Name="EnigmaHost" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="{Binding ElementName=DockPanel1, Path=ActualHeight}" Width="{Binding ElementName=DockPanel1, Path=ActualWidth}">
        </WindowsFormsHost>
        </DockPanel>
</Window>
